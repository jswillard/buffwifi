doctype html
head
  // Required meta tags
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
  meta(name='viewport' content='initial-scale=1,maximum-scale=1,user-scalable=yes')
  script(src='https://api.tiles.mapbox.com/mapbox-gl-js/v1.5.0/mapbox-gl.js')
  // Bootstrap CSS
  link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css' integrity='sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO' crossorigin='anonymous')
  link(href='https://api.tiles.mapbox.com/mapbox-gl-js/v1.5.0/mapbox-gl.css' rel='stylesheet')
  title Suggestive Lettuce Speed Test
style.
  body { margin: 0; padding: 0; }
  #map { position: absolute; top: 0; bottom: 0; width: 100%; };
br
.container
  h1.text-white Suggestive Lettuce Wi-Fi Speed Test
br
.container
  .row
    .col-8
      table.table.shadow.p-3.mb-5.rounded
        thead.thead-dark
          tr
            th(scope='col' colspan='4') Your speed test results
        tbody
          tr
            th.bg-white.text-dark(scope='row') Download
            td.text-white ?
            th.bg-white.text-dark Upload
            td.text-white ?
          tr
            th.bg-white.text-dark(scope='row') Location
            td.text-white ?
            th.bg-white.text-dark Ping
            td.text-white ?
          tr
            th.bg-white.text-dark(scope='row') Date
            td.text-white ?
            th.bg-white.text-dark Time
            td.text-white ?
    .col-4.text-center
      img.rounded(src='../CSCI-3308/PicsArt_11-07-11.01.54.png' align='middle' alt='logo' height='200' width='150')
  br
  br
.container.shadow.p-3.mb-5.bg-white.rounded
  ul#myTab.nav.nav-tabs(role='tablist')
    li.nav-item
      a#charts-tab.nav-link.active(data-toggle='tab' href='#charts' role='tab' aria-controls='charts' aria-selected='true') Chart View
    li.nav-item
      a#compare-tab.nav-link(data-toggle='tab' href='#compare' role='tab' aria-controls='compare' aria-selected='false') Compare
    li.nav-item
      a#best-tab.nav-link(data-toggle='tab' href='#best' role='tab' aria-controls='best' aria-selected='false') Best and Worst
  #myTabContent.tab-content
    #charts.tab-pane.fade.show.active(role='tabpanel' aria-labelledby='charts-tab')
      | ...
    #compare.tab-pane.fade(role='tabpanel' aria-labelledby='suggestion-tab')
      | ...
    #best.tab-pane.fade(role='tabpanel' aria-labelledby='best-tab')
      | ...
br
.container(height='700px')
  iframe(src='../CSCI-3308/maptesting.html' width='100%' height='700px')
  | &gt;
br
br
br
